   "source": [
    "# addition will takes place after multiplication and division\n",
    "num1 = 1 + 4 * 3 / 2;\n",
    "\n",
    "# same as 5 * 3 /2 \n",
    "num2 = (1 + 4) * 3 / 2;\n",
    "\n",
    "# same as 1+12/2 \n",
    "num3 = 1 + (4 * 3) / 2;\n",
    "\n",
    "print(\"python follow precedence rules\");\n",
    "\n",
    "# this should produce 7.5\n",
    "print(num1);\n",
    "print(num2);\n",
    "print(num3);"
   ]
  }
